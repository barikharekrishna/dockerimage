 pipeline {
  agent {
    kubernetes {
  stages {         
    stage("Git Checkout"){           
      steps{                
	checkout([$class: 'GitSCM', branches: [[name: '*/main']], extensions: [], userRemoteConfigs: [[url: 'https://github.com/barikharekrishna/dockerimage.git']]])                                  
	echo 'Git Checkout Completed'            
      }        
    }
  }
}
